
@{
    ViewBag.Title = "Deal";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="page-wrapper" style="width:96%; margin-top:20px;">
    <div class="page-content" style="padding-bottom:0px;">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-body">
                            <button type="button" class="btn btn-gradient-primary px-4 mt-0 mb-3" onclick="location.href='/Admin/AddDeal'"><i class="mdi mdi-plus-circle-outline mr-2"></i>New</button>
                            <table class="table table-striped table-bordered dt-responsive nowrap" id="tbldeal" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                <thead>
                                    <tr>
                                        <th>Deal Id</th>
                                        <th>Opportunity Name</th>
                                        <th>Customer Name</th>
                                        <th>City</th>
                                        <th>Uploaded Date</th>
                                        <th>Approval Status</th>
                                        <th>Till Date</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div> <!-- end col -->
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/jquery-3.6.0.min.js"></script>
<script src="~/Scripts/jquery-3.6.0.js"></script>
<script src="~/Scripts/View/Common/Common.js"></script>
<script src="~/Scripts/View/Admin/Deals.js"></script>
