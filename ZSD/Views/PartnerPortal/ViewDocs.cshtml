
@{
    ViewBag.Title = "ViewDocs";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="page-wrapper" style="width:100%;">
    <div class="page-content" style="padding-bottom:0px;">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12 p-3">
                    <div class="card">
                        <div class="card-body">
                            <button type="button" class="btn btn-gradient-primary px-4 mt-0 mb-3" onclick="location.href='/PartnerPortal/AddDocs'"><i class="mdi mdi-plus-circle-outline mr-2"></i>New</button>
                            <table id="tbldocs" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                <thead>
                                    <tr>
                                        <th>Id</th>
                                        <th>Document Name</th>
                                        <th>Category</th>
                                        <th>Description</th>
                                        <th>Uploaded Date</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div> <!-- end col -->
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/jquery-3.6.0.min.js"></script>
<script src="~/Scripts/jquery-3.6.0.js"></script>
<script src="~/Scripts/View/Common/Common.js"></script>
<script src="~/Scripts/View/Register/Mraketing_Docs.js"></script>