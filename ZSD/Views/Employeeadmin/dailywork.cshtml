        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="col-md-9"></div>
                            <div class="col-md-3 mb-3 float-right">
                                <select class="select2 form-control mb-3 custom-select select2-hidden-accessible" id="ddlemployee" style="width: 100%; height:36px;" tabindex="-1" aria-hidden="true">
                                </select>
                            </div>
                            <table class="table table-striped table-bordered dt-responsive nowrap" id="tblactivity" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                <thead>
                                    <tr>
                                        <th>Date</th>
                                        <th>Daily Activity</th>
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
