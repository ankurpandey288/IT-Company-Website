
@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_LayoutCompany.cshtml";
}

<div class="page-wrapper" style="width:100%; margin-top:20px;">
    <div class="page-content" style="padding-bottom:0px;">
        <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                    <div class="card">
                        <div class="card-body">
                            <table class="table table-striped table-bordered dt-responsive nowrap" id="tblduser" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                <thead>
                                    <tr>
                                        <th>User Id</th>
                                        <th>Email Id</th>
                                        <th>Designation</th>
                                        <th>Mobile</th>
                                        <th>Address</th>
                                        <th>City</th>
                                        <th>Country</th>
                                        <th>Approved By</th>
                                        @*<th>Creater</th>*@

                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/jquery-3.6.0.min.js"></script>
<script src="~/Scripts/jquery-3.6.0.js"></script>
<script src="~/Scripts/View/Common/Common.js"></script>
<script src="~/Scripts/View/company/users.js"></script>